@model IdentityRole
@{
    ViewData["Title"] = "Details";
}

<div class="block-header">
    <h2>Details</h2>
</div>

<form asp-action="Details" asp-controller="Roles" class="form-group">
    <div asp-validation-summary="All" class="bg-danger"></div>

    <div class="form-group">
        <lable> ID :</lable>
        <input type="text" class="form-control" asp-for="Id" readonly />
    </div>

    <div class="form-group">
        <lable> User Name :</lable>
        <input type="text" class="form-control" asp-for="Name"  disabled/>
    </div>






    <center>
        <input type="submit" value="Edite" class="btn btn-primary" />
        <input type="reset" value="Cancle" class="btn btn-danger" />
        <a asp-action="Index" asp-controller="Roles" class="btn btn-warning">Back To Data</a>
    </center>
</form>
@section jqueryValidation{

    <script src="~/plugins/jquery-validation/jquery.validate.js"></script>
    <script src="~/plugins/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>
}

@section BootstrapSelect{
    <link href="~/plugins/bootstrap-select/css/bootstrap-select.css" rel="stylesheet" />
}
