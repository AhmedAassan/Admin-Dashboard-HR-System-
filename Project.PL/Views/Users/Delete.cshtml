@model IdentityUserExtend
@{
    ViewData["Title"] = "Delete";
}

<div class="block-header">
    <h2>Delete  User</h2>
</div>

<form asp-action="Delete" asp-controller="Users" class="form-group">

    <div class="form-group">
        <lable> ID :</lable>
        <input type="text" class="form-control" asp-for="Id" readonly />
    </div>

    <div class="form-group">
        <lable> User Name :</lable>
        <input type="text" class="form-control" asp-for="UserName" readonly/>
    </div>

    <div class="form-group">
        <lable> Email :</lable>
        <input type="email" class="form-control" asp-for="Email" readonly/>
    </div>






    <center>
        <input type="submit" value="Delete" class="btn btn-primary" />
        <input type="reset" value="Cancle" class="btn btn-danger" />
        <a asp-action="Index" asp-controller="Users" class="btn btn-warning">Back To Data</a>
    </center>
</form>
@section jqueryValidation{

    <script src="~/plugins/jquery-validation/jquery.validate.js"></script>
    <script src="~/plugins/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>
}

@section BootstrapSelect{
    <link href="~/plugins/bootstrap-select/css/bootstrap-select.css" rel="stylesheet" />
}
